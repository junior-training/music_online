CREATE TABLE `tb_music_intro` (
  `id` int(11) NOT NULL,
  `song_intro_addr` varchar(45) DEFAULT NULL,
  `singer_intro_addr` varchar(45) DEFAULT NULL,
  `album_intro_addr` varchar(45) DEFAULT NULL,
