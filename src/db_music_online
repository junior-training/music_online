CREATE TABLE `tb_music` (
  `id` int(11) NOT NULL AUTO_INCREMENT COMMENT '标识歌曲唯一的id',
  `song_name` varchar(100) DEFAULT NULL COMMENT '歌曲名称',
  `singer_name` varchar(100) DEFAULT NULL COMMENT '歌手名称',
  `album_name` varchar(100) DEFAULT NULL COMMENT '专辑名称',
  `song_format` varchar(100) DEFAULT NULL COMMENT '歌曲格式，即后缀名',
  `song_addr` varchar(100) DEFAULT NULL COMMENT '歌曲地址',
  `image_addr` varchar(100) DEFAULT NULL COMMENT '播放时使用的图片地址，如专辑地址或一张歌手的图片 ',
  `lyric_addr` varchar(100) DEFAULT NULL COMMENT '歌词地址 ',
  `song_genre` varchar(100) DEFAULT NULL COMMENT '歌曲曲风，如华语，欧美与日韩',
  PRIMARY KEY (`id`)
) ENGINE=InnoDB AUTO_INCREMENT=115 DEFAULT CHARSET=utf8 COMMENT='存储歌曲的基本元素信息';
